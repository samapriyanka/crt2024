// read 2 no's and print 1 for add,2 for sub,3 for mul and 4 for div
#include <stdio.h>

int main() {
    // Write C code here
    int a,b; 
    scanf("%d%d",&a,&b);
    int ch;
    scanf("%d",&ch);
    switch(ch)
    {
        case 1:
          printf("%d",a+b);
        break;
        case 2:
           printf("%d",a-b);
        break;
        case 3:
           printf("%d",a*b);
        break;
        case 4:
           printf("%d",a/b);
        break;
        default :
           printf("invalid choice");
    }
    
    return 0;
}
    
